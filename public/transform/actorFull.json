{
    "name": {
        "mappingName": "nameinfo",
        "type": "array",
        "item": {
            "type": "object"
        },
        "filter": [
            {
                "key": "nametype",
                "values": [
                    "原名",
                    "chs"
                ]
            }
        ],
        "result": {
            "count": 1,
            "type": "object"
        }
    },
    "photo": {
        "mappingName": "photoinfo",
        "type": "array",
        "item": {
            "type": "array",
            "item": {
                "type": "object"
            }
        },
        "result": {
            "type": "array",
            "contactSubArrays": true
        },
        "display": {
            "label": "相册",
            "type": "imageSlide",
            "imageColumn": "imgPath"
        }
    },
    "nameinfo": {
        "mappingName": "nameinfo",
        "type": "array",
        "item": {
            "type": "object"
        },
        "result": {
            "type": "array"
        },
        "display": {
            "label": "称谓",
            "type": "table",
            "showHeader": false,
            "columns": [
                {
                    "column": "nametype",
                    "dataConvertion": {
                        "en": "英文名",
                        "cht": "繁体名",
                        "chs": "简体名"
                    },
                    "style": "font-size:8px;white-space:normal"
                },
                {
                    "column": "namelabel",
                    "style": "font-size:8px;white-space:normal"
                }
            ]
        }
    },
    "gender": {
        "mappingName": "gender",
        "type": "string",
        "result": {
            "type": "string"
        },
        "display": {
            "label": "性别",
            "type": "label"
        }
    },
    "birthday": {
        "mappingName": "birthday",
        "type": "string",
        "result": {
            "type": "string"
        },
        "display": {
            "label": "出生日期",
            "type": "label"
        }
    },
    "briefBiography": {
        "mappingName": "briefBiography",
        "type": "array",
        "item": {
            "type": "string"
        },
        "result": {
            "type": "array"
        },
        "display": {
            "label": "简介",
            "type": "table",
            "showHeader": false,
            "columns": [
                {
                    "column": "singleValue",
                    "style": "font-size:8px;white-space:normal"
                }
            ]
        }
    },
    "createdWork": {
        "mappingName": "createdWork",
        "type": "array",
        "item": {
            "type": "string"
        },
        "result": {
            "type": "array"
        },
        "display": {
            "label": "作品集",
            "type": "table",
            "showHeader": false,
            "columns": [
                {
                    "column": "singleValue",
                    "style": "font-size:8px;white-space:normal"
                }
            ]
        }
    },
    "noteOfSource": {
        "mappingName": "noteOfSource",
        "type": "array",
        "item": {
            "type": "string"
        },
        "result": {
            "type": "array"
        },
        "display": {
            "label": "资料来源",
            "type": "table",
            "showHeader": false,
            "columns": [
                {
                    "column": "singleValue",
                    "style": "font-size:8px;white-space:normal"
                }
            ]
        }
    },
    "relatedOrganization": {
        "mappingName": "relatedOrganization",
        "type": "array",
        "item": {
            "type": "string"
        },
        "result": {
            "type": "array"
        },
        "display": {
            "label": "从属机构",
            "type": "table",
            "showHeader": false,
            "columns": [
                {
                    "column": "singleValue",
                    "style": "font-size:8px;white-space:normal"
                }
            ]
        }
    }
}